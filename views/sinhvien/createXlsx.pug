extends ../commons/sinhvien.common.pug

block content
    h1.text-center.my-4 Thêm sinh viên bằng file XLSX

    form.m-5(action="/sinhvien/addXlsx", method="POST", enctype="multipart/form-data")
        .form-group
            label(for="xlsxdata") Chọn file Xlsx
            input#xlsxdata.form-control-file(type="file", accept=".xlsx", name="data")
        .text-right
            button.btn.btn-primary(type="submit") Thêm
    
    a.btn.btn-secondary.ml-4(href="/") Back
